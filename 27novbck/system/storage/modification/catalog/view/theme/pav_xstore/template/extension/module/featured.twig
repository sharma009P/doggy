<div class="category-bg">
    <img src="./image/category_bg.jpg" class="img-responsive" />
    <div class="custom_container category_page_title">
        <div class="col-md-12">
            <h2>Dog Collars &amp; Leads</h2>
        </div>
    </div>
</div>

<div class="custom_container">
    <div class="product-featured col-nopadding">

				{% if module_wgtm_status %}
					{% if wgtms and wgtms %}
					{% set position = 1 %}
					<script>
						dataLayer.push({
							'event':'enhanceEcom productImpression',
							'ecommerce': {
								'currencyCode': "{{ wcurencysymbol }}",
								'impressions': [
									{% for wgtm in wgtms %}
									{% if position > 1 %},{% endif %}{
									'name': "{{ wgtm['name'] }}",
									'id': "{{ wgtm['product_id'] }}",
									'price': "{{ wgtm['price'] }}",
									'brand': "{{ wgtm['manufacturer'] }}",
									'category': "{{ wgtm['categories'] }}",
									'list': "{{ wmodulename }}",
									'position': "{{ position }}"
									}
									{% set position = position + 1 %}
									{% endfor %}
								]
							}
						});
					</script>
					{% endif %}
				{% endif %}
			
      <div class="row">
       {% for product in products %}
        <div class="product-layout col-lg-3 col-md-3 col-sm-6 col-xs-12">
          <div class="product-thumb transition">
            <div class="image"><a 
				href="{{ product.href }}" {% if module_wgtm_status %} onclick="productclick('{{ product['product_id'] }}', '{{ product['href'] }}'); return false" {% endif %}
			><img src="{{ product.thumb }}" alt="{{ product.name }}" title="{{ product.name }}" class="img-responsive" /></a></div>
            <div class="product-meta clearfix">
              <div class="caption">
                <h4 class="name"><a 
				href="{{ product.href }}" {% if module_wgtm_status %} onclick="productclick('{{ product['product_id'] }}', '{{ product['href'] }}'); return false" {% endif %}
			>{{ product.name }}</a></h4>
                <h5><a href="{{product.sellerhref}}">By {{product.sellername}}</a></h5>
                <!--<p class="description">{{ product.description }}</p>-->
                {% if product.rating %}
                
                 <div class="rating-custom">
                    {% for i in 1..5 %} 

                        {% if product.rating < i %} <span class="fa fa-stack"><i class="fa fa-star-o fa-stack-1x"></i></span> {% else %} <span class="fa fa-stack"><i class="fa fa-star fa-stack-1x"></i><i class="fa fa-star-o fa-stack-1x"></i></span> {% endif %}
            
                  {% endfor %}
                 </div>
      
                <!--<div class="rating">
                  {% for i in 5 %}
                  {% if product.rating < i %}
                  <span class="fa fa-stack"><i class="fa fa-star-o fa-stack-1x"></i></span>
                  {% else %}
                  <span class="fa fa-stack"><i class="fa fa-star fa-stack-1x"></i><i class="fa fa-star-o fa-stack-1x"></i></span>
                  {% endif %}
                  {% endfor %}
                </div>-->
                {% endif %}
                <div class="group-action clearfix">
                  {% if product.price %}
                  <div class="price">
                    {% if not product.special %}
                    {{ product.price }}
                    {% else %}
                    <span class="price-new">{{ product.special }}</span> <!--<span class="price-old">{{ product.price }}</span>-->
                    {% endif %}
                  </div>
                  {% endif %}
                  <!--<div class="button-group">
                    <button class="btn btn-cart" type="button" 
				onclick="cart.add('{{ product['product_id'] }}'); {% if module_wgtm_status %} waddtocart('{{ product['product_id'] }}'); {% endif %}"
			);"><i class="fa fa-shopping-cart"></i> <span class="hidden-xs hidden-sm hidden-md">{{ button_cart }}</span></button>
                    <button class="btn btn-wishlist" type="button" data-toggle="tooltip" title="{{ button_wishlist }}" onclick="
				wishlist.add('{{ product['product_id'] }}'); {% if module_wgtm_status %} waddtowishlist('{{ product['product_id'] }}');{% endif %}"
			"><i class="fa fa-heart"></i></button>
                    <button class="btn btn-compare" type="button" data-toggle="tooltip" title="{{ button_compare }}" onclick="
				compare.add('{{ product['product_id'] }}'); {% if module_wgtm_status %} waddtocompare('{{ product['product_id'] }}');{% endif %}"
			"><i class="fa fa-exchange"></i></button>
                  </div>-->
                </div>
              </div>
            </div>
          </div>
        </div>
        
        {% endfor %}
      </div>
    </div>
    
    <div class="row featured_product_footer_text">
        <div class="col-md-4 col-xs-12">
            <img src="./image/doggy_dmo.jpg" class="img-responsive" />
            <h3><a href="#">Lorem ipsum</a></h3>
        </div>
        <div class="col-md-4 col-xs-12">
            <img src="./image/doggy_dmo.jpg" class="img-responsive" />
            <h3><a href="#">Lorem ipsum</a></h3>
        </div>
        <div class="col-md-4 col-xs-12">
            <img src="./image/doggy_dmo.jpg" class="img-responsive" />
            <h3><a href="#">Lorem ipsum</a></h3>
        </div>
    </div>
    
</div>

